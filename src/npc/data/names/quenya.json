{
    "ref": ["https://eldamo.org/content/language-pages/lang-q.html"],
    "rules": [
        {
            "regex": "ve$",
            "repl": "we",
            "descr": "[ve] became [we]"
        },
        [
            "short-vowels-vanished-before-morpheme-boundaries",
            { "prob": 0.7 }
        ],
        "double-vowels-to-single-vowel"
    ],
    "diacritics": [
        "not-silent-final-e",
        {
            "regex": "\\Be([ao])",
            "repl": "ë$1",
            "descr": "vowel combinations don't form diphthongs"
        },
        {
            "regex": "([io])e",
            "repl": "$1ë",
            "descr": "vowel combinations don't form diphthongs"
        },
        {
            "regex": "^ea",
            "repl": "eä",
            "descr": "[ëa] is capitalized to [Eä]"
        }
    ],
    "gender": [
        {
            "type": "male",
            "regex": "([^aeioun])$",
            "repl": ["$1o", "$1ion", "$1indo", "$1mo", "$1ro"],
            "prob": 0.3
        },
        {
            "type": "male",
            "regex": "([aieu]+)$",
            "repl": ["o", "ion", "indo", "$1mo", "$1ro"],
            "prob": 0.3
        },
        {
            "type": "female",
            "regex": "([^aeioul])$",
            "repl": ["$1e", "$1iel", "$1inde", "$1me", "$1re"],
            "prob": 0.3
        },
        {
            "type": "female",
            "regex": "([aiou]+)$",
            "repl": ["e", "iel", "inde", "$1me", "$1re"],
            "prob": 0.3
        }
    ],

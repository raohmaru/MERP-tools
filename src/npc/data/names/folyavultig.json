{
    "rules": [
        "double-vowels-to-single-vowel",
        {
            "regex": "[ou](?=[^aeiou]*$)",
            "repl": {
                "o": "ö",
                "u": "ü"
            },
            "prob": 0.8,
            "descr": "last [o], [u] become front vowels"
        },
        [
            "acute-accent-to-vowel-in-last-syllable-ending-in-consonant",
            { "prob": 0.5 }
        ],
        [
            "acute-accent-to-vowel-in-next-to-last-syllable",
            { "prob": 0.5 }
        ]

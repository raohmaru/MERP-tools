{
    "ref": ["https://eldamo.org/content/language-pages/lang-n.html"],
    "rules": [
        "short-i-u-became-e-o-preceding-final-a",
        [
            "short-u-became-o",
            { "prob": 0.4 }
        ],
        "short-e-o-became-i-u-in-syllable-before-final-i",
        {
            "regex": "([^aeiou]|^)a([^aeiou]+i)",
            "repl": "$1e$2",
            "descr": "short [a] became [e] preceding [i]"
        },
        "short-vowels-vanished-before-morpheme-boundaries",
        "double-vowels-to-single-vowel",
        {
            "regex": "mm",
            "repl": "m",
            "prob": 0.4,
            "descr": "[mm] often shortened"
        },
        {
            "regex": "n(m|b)",
            "repl": "m$1",
            "descr": "[nm], [nb] became [mm], [mb]"
        },
        {
            "regex": "ls",
            "repl": "lth",
            "descr": "[ls] became [lth]"
        },
        {
            "regex": "rs",
            "repl": "ss",
            "descr": "[rs] became [ss]"
        },
        {
            "regex": "\\B(?<!m)m(?![bmp]\\B)",
            "repl": "v",
            "prob": 0.3,
            "descr": "non-initial [m] usually became [v]"
        },
        {
            "regex": "vn",
            "repl": "von",
            "prob": 0.5,
            "descr": "final [vn] sometimes became [von]"
        }
    ],
    "gender": [
        {
            "type": "male",
            "regex": "([^aeioun])$",
            "repl": ["$1o", "$1ion", "$1indo", "$1mo", "$1ro"],
            "prob": 0.3
        },
        {
            "type": "male",
            "regex": "([aieu]+)$",
            "repl": ["o", "ion", "indo", "$1mo", "$1ro"],
            "prob": 0.3
        },
        {
            "type": "female",
            "regex": "([^aeioul])$",
            "repl": ["$1e", "$1iel", "$1inde", "$1me", "$1re"],
            "prob": 0.3
        },
        {
            "type": "female",
            "regex": "([aiou]+)$",
            "repl": ["e", "iel", "inde", "$1me", "$1re"],
            "prob": 0.3
        }
    ],

{
    "rules": [
        "double-vowels-to-single-vowel",
        "not-silent-final-e",
        {
            "regex": "nn",
            "repl": "n’n",
            "descr": "glottal stop between [nn]"
        },
        [
            "add-circumflex-to-vowel-in-next-to-last-syllable",
            { "prob": 0.5 }
        ],
        {
            "regex": "[aiou](?=[^aeiou]*$)",
            "repl": {
                "a": "â",
                "i": "î",
                "o": "ô",
                "u": "û"
            },
            "prob": 0.8,
            "descr": "add circumflex to last [aiou]"
        }
    ],

{
    "rules": [{
            "regex": "([aeiou])\\1+",
            "repl": "$1$1",
            "descr": "triple vowels to double vowel"
        },
        {
            "regex": "(.+)a\\B",
            "repl": "$1’a",
            "prob": 0.2,
            "descr": "glottal stop before [a]"
        },
        {
            "regex": "a([^’]+)\\B",
            "repl": "a’$1",
            "prob": 0.2,
            "descr": "glottal stop after [a]"
        }
    ],
